-- draw

function _draw()
 cls(1)
 camera(0,cam_y)
 map()

 pspr=16
 if(player.dirt) pspr=17
--	spr(pspr,player.x,player.y)
	spr(pspr,player.x-player.w/2,player.y)---player.h/2+1)
 circfill(player.x,player.y,1)
 if sel_x != -1 then
  spr(32,sel_x*8,sel_y*8) 
 end	
end
-->8
--update
player={}
player.x=64
player.y=48
player.w=8
player.h=8
player.vy=0
player.dirt=false
joejustjumped=0
joewasonground=0
cam_y=-8
sel_x=-1
sel_y=-1

function _update()
 local jumping = btnp(â¬†ï¸)
 local prev_selx,prev_sely=sel_x,sel_y

 if btn(ðŸ…¾ï¸) then
  local sx=flr(player.x/8)
  local sy=flr(player.y/8)
  local dsx,dsy=0,0
  if(btn(â¬‡ï¸)) dsy+=1
  if(btn(â¬†ï¸)) dsy-=1
  if(btn(âž¡ï¸)) dsx+=1
  if(btn(â¬…ï¸)) dsx-=1
  if dsx==0 and dsy==0 then
   sel_x,sel_y=-1,-1
  else
	  sel_x=sx+dsx
	  sel_y=sy+dsy
	 end
 else
  sel_x,sel_y=-1,-1
 end
 
	if sel_x == -1 then 
	 if prev_selx ~= -1 then
	   if not player.dirt then
	    if floortouch(player) then
--		    local mx=player.x/8
--		    local my=player.y/8+1
		    try_dig(prev_selx,prev_sely)
		   end
		  else
		   -- dump
--	    local mx=player.x/8
--	    local my=player.y/8+1
		    try_dump(prev_selx,prev_sely)
					 if prev_sely==flr(player.y/8) and
					    prev_selx==flr(player.x/8) and
					  floortouch(player) then
					  jumping = true
					 end
	   end
	 end
	
	 local nx, ny = 0,0
	 local ft=floortouch(player)
		 if(btn(â¬…ï¸)) nx = -1
		 if(btn(âž¡ï¸)) nx = 1
	 if ft then
	  player.vy=0
	  player.y-=player.y%8
	  if(jumping) player.vy=-2
	 else
	  player.vy=player.vy+0.3
		end 
		ny = player.vy
		
	 player.x += nx
	 player.y += ny
	 if wallcoll(player) then
	 	player.x -= nx
	 end 
	end
	
 cam_y = player.y - 64
end

function try_dig(mx,my)
 local t=mget(mx,my)
 if(t==0) return
 if fget(t,0) then
  sfx(0)
 else
  sfx(1)
	 mset(mx,my,0)
	 player.dirt=true
 end
end

function try_dump(mx,my)
 local t=mget(mx,my)
 if(t~=0) return
 mset(mx,my,1)
 sfx(2)
 player.dirt=false
end

function floortouch(act)
 local x,y,t
 local w,h=player.w, player.h
 x,y=player.x-w/2+1,(player.y)/8+1
 t=mget(x/8,y)
 if (t>0) return true
 x,y=player.x+w/2-1,(player.y)/8+1
 t=mget(x/8,y)
 if (t>0) return true
 return false
end

function wallcoll(act)
 local x,y,t
 local w,h=player.w, player.h
 x,y=player.x-w/2,player.y
 t=mget(x/8,y/8)
 if (t>0) return true
 x,y=player.x+w/2,player.y
 t=mget(x/8,flr(y/8))
 if (t>0) return true
 x,y=player.x+w/2,player.y+h-1
 t=mget(x/8,y/8)
 if (t>0) return true
 x,y=player.x-w/2,player.y+h-1
 t=mget(x/8,y/8)
 if (t>0) return true
 
 if(player.x<=0 or player.x>=128) return true
 
 return false
end

