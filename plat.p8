version 18
__lua__
local player={}
player.x=28
player.y=80
player.vy=0
player.w=5
player.h=8

function _update()
 local nx, ny = 0,0
 if(btn(0)) nx = -1
 if(btn(1)) nx = 1
 player.x += nx
 player.y += ny
 if wallcoll(player) then
 	player.x -= nx
 	player.y -= ny
 end 
end

function wallcoll(act)
 local x,y,t
 local w,h=player.w, player.h
 x,y=player.x-w/2,player.y-h/2
 t=mget(x/8,y/8)
 if (t>0) return true
 x,y=player.x+w/2,player.y-h/2
 t=mget(x/8,y/8)
 if (t>0) return true
 x,y=player.x+w/2,player.y+h/2
 t=mget(x/8,y/8)
 if (t>0) return true
 x,y=player.x-w/2,player.y+h/2
 t=mget(x/8,y/8)
 if (t>0) return true
 return false
end

function _draw()
 cls()
 map()
	spr(1,player.x-player.w/2,player.y)
end


