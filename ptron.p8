 local d=0
 if (t==curr_wpn().open) d=1
 -- on weapon
  --todo neighbors too  
	if (d>0) open_neigh(rx,ry,t)
 b.x -= b.spd.x
 b.y -= b.spd.y	
end

function open_neigh(rx,ry,t)
  if mget(rx,ry)==t then  
			mset(rx,ry,65)
			open_neigh(rx-1,ry,t)
			open_neigh(rx+1,ry,t)
 		open_neigh(rx,ry-1,t)
 		open_neigh(rx,ry+1,t)			
		end
end

