 room_trans_t=room_trans_time
 set_room(nroom)
 on_door = true
 dr_ene_unlocked = false
 if pl.y>=scr.y1-8 then
 	pl.y = scr.y0+4
 	room_trans_d={x=0,y=1}
 elseif pl.y <= scr.y0+8 then
  pl.y = scr.y1-4
 	room_trans_d={x=0,y=-1}
 end
 if pl.x>=120 then
 	pl.x = 4
 	room_trans_d={x=1,y=0}
